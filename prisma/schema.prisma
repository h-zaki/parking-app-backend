model Parking {
  id            Int      @id @default(autoincrement())
  name          String
  emptyBlocks   Int
  city          String
  price         Int
  adress        String
  description   String
  img           String
}














generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
